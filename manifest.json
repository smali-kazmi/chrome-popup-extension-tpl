{
    "manifest_version": 2,
    "name": "Chrome Popup Extension Template",
    "description": "A simple Google Chrome Popup Extension Template for starters",
    "version": "1.0",
    "browser_action": {
        "default_icon": "images/icon-32.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "http://*/",
        "https://*/",
        "<all_urls>"
    ]
}
